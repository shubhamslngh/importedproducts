{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "paths": {
      "#imports": [
        "./types/nitro-imports"
      ],
      "#paths": [
        "../../node_modules/nuxt/dist/core/runtime/nitro/paths"
      ],
      "~": [
        "./.."
      ],
      "~/*": [
        "../*"
      ],
      "@": [
        "./.."
      ],
      "@/*": [
        "../*"
      ],
      "~~": [
        "./.."
      ],
      "~~/*": [
        "../*"
      ],
      "@@": [
        "./.."
      ],
      "@@/*": [
        "../*"
      ],
      "assets": [
        "../assets"
      ],
      "public": [
        "../public"
      ],
      "#apollo": [
        "./apollo"
      ],
      "#icon-collections": [
        "./icon-collections"
      ],
      "#color-mode-options": [
        "./color-mode-options"
      ],
      "pinia": [
        "../../node_modules/pinia/dist/pinia"
      ],
      "#mdc-configs": [
        "./mdc-configs"
      ],
      "#mdc-highlighter": [
        "./mdc-highlighter"
      ],
      "#mdc-imports": [
        "./mdc-imports"
      ],
      "#content/server": [
        "../../node_modules/@nuxt/content/dist/runtime/legacy/server"
      ]
    }
  },
  "include": [
    "./types/nitro-nuxt.d.ts",
    "../../modules/awesome.ts/runtime/server",
    "../../node_modules/vue3-carousel-nuxt/runtime/server",
    "../../node_modules/@nuxtjs/apollo/runtime/server",
    "../../node_modules/@nuxtjs/tailwindcss/runtime/server",
    "../../node_modules/@nuxtjs/eslint-module/runtime/server",
    "../../node_modules/nuxt-headlessui/runtime/server",
    "../../node_modules/nuxt-icon/runtime/server",
    "../../node_modules/@nuxtjs/color-mode/runtime/server",
    "../../node_modules/@pinia-plugin-persistedstate/nuxt/runtime/server",
    "../../node_modules/@pinia/nuxt/runtime/server",
    "../../node_modules/@vueuse/nuxt/runtime/server",
    "../../node_modules/@nuxtjs/mdc/runtime/server",
    "../../node_modules/@nuxt/content/runtime/server",
    "../../node_modules/@nuxt/devtools/runtime/server",
    "../../node_modules/@nuxt/telemetry/runtime/server",
    "./types/nitro.d.ts",
    "../**/*",
    "../server/**/*"
  ],
  "exclude": [
    "../node_modules",
    "../../node_modules",
    "../../node_modules/nuxt/node_modules",
    "../dist"
  ]
}
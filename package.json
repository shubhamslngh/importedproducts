{
    "name": "@nuxt-awesome/theme",
    "version": "2.0.12",
    "description": "a Nuxt 3 template and boilerplate with a lot of useful features. Nuxt 3 + Tailwindcss + Nuxt Layer",
    "author": "viandwi24",
    "private": false,
    "license": "MIT",
    "type": "module",
    "main": "./nuxt.config.ts",
    "repository": {
        "type": "git",
        "url": "git://github.com/viandwi24/nuxt3-awesome-starter"
    },
    "files": [
        "assets",
        "components",
        "composables",
        "layouts",
        "modules",
        "pages",
        "public",
        "stores",
        "app.config.ts",
        "app.vue",
        "nuxt.config.ts",
        "nuxt.schema.ts",
        "tailwind.config.ts",
        "tsconfig.json"
    ],
    "scripts": {
        "serve": "vue-cli-service serve --host 0.0.0.0",
        "build": "nuxt build ./app",
        "dev": "nuxt dev ./app --no-clear",
        "generate": "nuxt generate ./app",
        "preview": "nuxt preview ./app",
        "postinstall": "nuxt prepare ./app",
        "dev:starter": "nuxt dev ./examples/starter",
        "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
        "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
        "release": "release-it"
    },
    "dependencies": {
        "@nuxt/content": "^2.9.0",
        "@nuxt/devtools": "^1.0.0",
        "@nuxtjs/color-mode": "^3.3.0",
        "@nuxtjs/tailwindcss": "^6.11.4",
        "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
        "@pinia/nuxt": "^0.5.1",
        "@syncfusion/ej2-base": "^24.2.7",
        "@syncfusion/ej2-vue-navigations": "^24.2.8",
        "@vue/apollo-composable": "^4.0.2",
        "@vue/apollo-option": "^4.0.0",
        "@vue/cli-service": "^5.0.8",
        "@vueuse/core": "^10.5.0",
        "@vueuse/motion": "^2.1.0",
        "@vueuse/nuxt": "^10.5.0",
        "apollo-cache-inmemory": "^1.6.6",
        "axios": "^1.6.7",
        "clsx": "^2.1.0",
        "i": "^0.3.7",
        "jsonwebtoken": "^9.0.2",
        "npm": "^10.5.0",
        "nuxt-graphql-client": "^0.2.33",
        "nuxt-graphql-middleware": "^4.0.0",
        "nuxt-headlessui": "^1.1.4",
        "nuxt-icon": "^0.6.1",
        "nuxt-swiper": "latest",
        "pinia": "^2.1.7",
        "sass": "^1.69.5",
        "stringify": "^5.2.0",
        "swiper": "^11.1.0",
        "tailwind-merge": "^2.2.1",
        "unplugin-vue-components": "^0.25.2",
        "vue-360": "^0.1.6",
        "vue-resource": "^1.5.3",
        "vue-swatches": "^2.1.1"
    },
    "devDependencies": {
        "@nuxtjs/apollo": "^5.0.0-alpha.13",
        "@nuxtjs/eslint-config-typescript": "^12.1.0",
        "@nuxtjs/eslint-module": "^4.1.0",
        "consola": "^3.2.3",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "nuxt": "^3.10.3",
        "prettier": "^3.0.3",
        "release-it": "^16.2.1",
        "serve": "^14.2.1",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.2.2",
        "vue-tsc": "1.8.22",
        "vue3-carousel-nuxt": "^1.1.0"
    },
    "release-it": {
        "github": {
            "release": true,
            "releaseName": "v${version}"
        },
        "git": {
            "commitMessage": "chore(release): release v${version}"
        }
    }
}
